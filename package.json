{
  "name": "protector-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx ./src/server/index.ts",
    "build": "rimraf dist && tsc -p tsconfig.json && npm run build:client && npm run build:server && npm run copy-files",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr ./src/client/app/entry.server.tsx && vite build --ssr ./src/server/index.ts",
    "start": "npm run build && cross-env NODE_ENV=production node ./dist/index.js",
    "copy-files": "copyfiles -u 2 \"dist/client/assets/*\" dist/public && rimraf dist/client/assets && copyfiles -u 2 \"dist/client/**/*\" dist && rimraf dist/client"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "preact": "^10.16.0",
    "preact-render-to-string": "^6.3.0",
    "preact-router": "^4.1.2",
    "remove": "^0.1.5",
    "unistore": "^3.5.2"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.52.0",
    "eslint-config-preact": "^1.3.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
